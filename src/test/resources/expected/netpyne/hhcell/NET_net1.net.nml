<neuroml xmlns="http://www.neuroml.org/schema/neuroml2"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.neuroml.org/schema/neuroml2 https://raw.githubusercontent.com/NeuroML/NeuroML2/development/Schemas/NeuroML2/NeuroML_v2.1.xsd"
    id="Exported_from_LEMS">

<network id="net1">
    <population id="hhpop" component="hhcell" size="1"/>
    <explicitInput input="pulseGen1" target="hhpop[0]"/>
</network>

<cell id="hhcell">
    <morphology type="morphology" id="morph1">
        <segment id="0" name="soma">
            <proximal type="proximal" x="0" y="0" z="0" diameter="17.841242"/>
            <distal type="distal" x="0" y="0" z="0" diameter="17.841242"/>
        </segment>
        <segmentGroup id="Soma">
            <member segment="0"/>
        </segmentGroup>
        <segmentGroup id="soma_group">
            <include segmentGroup="Soma"/>
        </segmentGroup>
    </morphology>
    <biophysicalProperties type="biophysicalProperties" id="bioPhys1">
        <membraneProperties type="membraneProperties">
            <channelDensity id="leak" ionChannel="passive" condDensity="3.0 S_per_m2" erev="-54.3mV"/>
            <channelDensity id="naChans" ionChannel="na" condDensity="120.0 mS_per_cm2" erev="50.0 mV"/>
            <channelDensity id="kChans" ionChannel="k" condDensity="360 S_per_m2" erev="-77mV"/>
            <specificCapacitance value="1.0 uF_per_cm2"/>
            <spikeThresh type="spikeThresh" value="-20mV"/>
            <initMembPotential type="initMembPotential" value="-65mV"/>
        </membraneProperties>
        <intracellularProperties type="intracellularProperties">
            <resistivity value="0.03 kohm_cm"/>
        </intracellularProperties>
    </biophysicalProperties>
</cell>

<ionChannelPassive id="passive" name="Passive conductance" conductance="10pS">
    <notes type="notes"/>
</ionChannelPassive>

<ionChannelHH id="na" conductance="10pS" species="na">
    <gateHHrates id="m" instances="3">
        <forwardRate type="HHExpLinearRate" rate="1per_ms" midpoint="-40mV" scale="10mV"/>
        <reverseRate type="HHExpRate" rate="4per_ms" midpoint="-65mV" scale="-18mV"/>
    </gateHHrates>
    <gateHHrates id="h" instances="1">
        <forwardRate type="HHExpRate" rate="0.07per_ms" midpoint="-65mV" scale="-20mV"/>
        <reverseRate type="HHSigmoidRate" rate="1per_ms" midpoint="-35mV" scale="10mV"/>
    </gateHHrates>
    <notes type="notes"/>
</ionChannelHH>

<ionChannelHH id="k" conductance="10pS" species="k">
    <gateHHrates id="n" instances="4">
        <forwardRate type="HHExpLinearRate" rate="0.1per_ms" midpoint="-55mV" scale="10mV"/>
        <reverseRate type="HHExpRate" rate="0.125per_ms" midpoint="-65mV" scale="-80mV"/>
    </gateHHrates>
</ionChannelHH>

<pulseGenerator id="pulseGen1" delay="0ms" duration="100000000s" amplitude="0.08nA"/>

</neuroml>